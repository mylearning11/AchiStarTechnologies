node {		stage('Clone repository') {        /* Let's make sure we have the repository cloned to our workspace */        checkout scm		}		stage('Build image') {		docker.withRegistry('https://registry.example.com', 'dockerhub') {        def customImage = docker.build("mylearning11/myproject:${env.BUILD_ID}")        /* Push the container to the custom Registry */        customImage.push()		}	}}