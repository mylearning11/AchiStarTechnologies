node {		stage('Clone repository') {        /* Let's make sure we have the repository cloned to our workspace */        checkout scm		}		stage('Build image') {		docker.withRegistry('https://registry.hub.docker.com/repository/docker/mylearning11/myproject/', 'dockerhub') {        def customImage = docker.build("mylearning11/myproject:latest")        /* Push the container to the custom Registry */        customImage.push()		}	}}